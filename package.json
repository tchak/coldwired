{
  "name": "remix-router-turbo",
  "description": "@remix-run/router on rails",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/remix-router-turbo.cjs.js",
  "module": "./dist/remix-router-turbo.es.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/remix-router-turbo.es.js",
      "require": "./dist/remix-router-turbo.cjs.js"
    }
  },
  "version": "0.4.2",
  "keywords": [
    "remix",
    "router",
    "turbo",
    "stimulus"
  ],
  "scripts": {
    "dev": "vite app",
    "build": "run-s clean build:*",
    "build:vite": "vite build",
    "build:tsc": "tsc --emitDeclarationOnly",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "run-s lint:*",
    "lint:types": "tsc --noEmit",
    "lint:js": "eslint . --ext .ts,.tsx",
    "clean": "del dist coverage node_modules/.vite",
    "prepare": "pnpm build",
    "release": "release-it"
  },
  "dependencies": {
    "@remix-run/router": "0.2.0-pre.3",
    "just-throttle": "^4.0.1",
    "morphdom": "^2.6.1",
    "nanoid": "^4.0.0"
  },
  "devDependencies": {
    "@remix-run/node": "^1.6.4",
    "@testing-library/dom": "^8.14.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitest/ui": "^0.17.1",
    "c8": "^7.11.3",
    "del-cli": "^4.0.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "jsdom": "^20.0.0",
    "just-debounce-it": "^3.0.1",
    "msw": "^0.43.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "release-it": "^15.1.1",
    "tiny-invariant": "^1.2.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vitest": "^0.17.1"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@6.32.24",
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-redeclare": "off"
    },
    "overrides": [
      {
        "files": [
          "vite.config.js",
          "vitest.config.ts"
        ],
        "env": {
          "node": true
        }
      }
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "npm": {
      "skipChecks": true
    },
    "github": {
      "release": true
    }
  }
}
